    "mobile_no": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "province": {
      "type": "string",
      "required": true
    },
    "encoder": {
